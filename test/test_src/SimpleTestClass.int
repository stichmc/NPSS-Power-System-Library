#ifndef _SIMPLE_TEST_CLASS_
#define _SIMPLE_TEST_CLASS_

class SimpleTest extends Element {
	ElectricOutputPort EP_O;
	ElectricInputPort EP_I;
	real Ireal = 500.;
	real Iimag = 0.;
	real Pdes = 1000.; //kW
	real PFdes = 0.95;
	
	Independent ind_Ireal { 
		varName = "Ireal"; 
		autoSetup = TRUE;
		a = 10000.;
	}
	
	Independent ind_Iimag { 
		varName = "Iimag"; 
		autoSetup = TRUE;
		a = 10000.;
	}
	
	Dependent dep_Power{
		eq_rhs="EP_O.S.rRMS";
		eq_lhs="Pdes";
		autoSetup=TRUE;
	}
	
	Dependent dep_PF{
		eq_rhs="cos(EP_O.S.phase)";
		eq_lhs="PFdes";
		autoSetup=TRUE;
	}
}

#endif